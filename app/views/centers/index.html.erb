<p id="notice"><%= notice %></p>

<h1>Listing Centers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Lattitude</th>
      <th>Longitude</th>
      <th>Paper</th>
      <th>Plastic</th>
      <th>Metal</th>
      <th>Glass</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @centers.each do |center| %>
      <tr>
        <td><%= center.name %></td>
        <td><%= center.address %></td>
        <td><%= center.lattitude %></td>
        <td><%= center.longitude %></td>
        <td><%= center.paper %></td>
        <td><%= center.plastic %></td>
        <td><%= center.metal %></td>
        <td><%= center.glass %></td>
        <td><%= link_to 'Show', center %></td>
        <td><%= link_to 'Edit', edit_center_path(center) %></td>
        <td><%= link_to 'Destroy', center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

